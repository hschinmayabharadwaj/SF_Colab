# PostgreSQL Database Configuration
# Copy this file to .env and update with your actual credentials

# Option 1: Full DATABASE_URL (recommended)
# DATABASE_URL=postgresql://username:password@localhost:5432/sf_combined

# Option 2: Individual components (used if DATABASE_URL is not set)
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=sf_combined

# Application Configuration
SECRET_KEY=your-secret-key-here-change-in-production-please

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
